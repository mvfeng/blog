#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/新员工培训资料/学习随笔/gitb/mvfeng/bloghexo/node_modules/.pnpm/hexo-cli@4.3.1/node_modules/hexo-cli/bin/node_modules:/mnt/e/新员工培训资料/学习随笔/gitb/mvfeng/bloghexo/node_modules/.pnpm/hexo-cli@4.3.1/node_modules/hexo-cli/node_modules:/mnt/e/新员工培训资料/学习随笔/gitb/mvfeng/bloghexo/node_modules/.pnpm/hexo-cli@4.3.1/node_modules:/mnt/e/新员工培训资料/学习随笔/gitb/mvfeng/bloghexo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/新员工培训资料/学习随笔/gitb/mvfeng/bloghexo/node_modules/.pnpm/hexo-cli@4.3.1/node_modules/hexo-cli/bin/node_modules:/mnt/e/新员工培训资料/学习随笔/gitb/mvfeng/bloghexo/node_modules/.pnpm/hexo-cli@4.3.1/node_modules/hexo-cli/node_modules:/mnt/e/新员工培训资料/学习随笔/gitb/mvfeng/bloghexo/node_modules/.pnpm/hexo-cli@4.3.1/node_modules:/mnt/e/新员工培训资料/学习随笔/gitb/mvfeng/bloghexo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/hexo" "$@"
else
  exec node  "$basedir/../../bin/hexo" "$@"
fi
